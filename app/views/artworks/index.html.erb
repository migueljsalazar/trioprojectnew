    <style>
     #map {
       width: 1,350px;
       height: 500px;
     }
   </style>
   <script src="https://maps.googleapis.com/maps/api/js"></script>
   <script>
     function initialize() {
       var mapCanvas = document.getElementById('map');
       var mapOptions = {
         center: new google.maps.LatLng(25.801160, -80.199968),
         zoom: 16,
         mapTypeId: google.maps.MapTypeId.ROADMAP
       }
       var map = new google.maps.Map(mapCanvas, mapOptions)

       var locations = [];
         locations.push ( {name:"Kazki 1", latlng: new google.maps.LatLng(25.801199, -80.200104)})
         locations.push ( {name:"Kazki 2", latlng: new google.maps.LatLng(25.801439, -80.199953)})
         locations.push ( {name:"Kazki 3", latlng: new google.maps.LatLng(25.801607, -80.201799)})
         locations.push ( {name:"Kashink", latlng: new google.maps.LatLng(25.801516, -80.201640)})
         locations.push ( {name:"El Mac", latlng: new google.maps.LatLng(25.802260, -80.205127)})
         locations.push ( {name:"Banksy", latlng: new google.maps.LatLng(25.801275, -80.199570)})

       for(var i=0;i<locations.length;i++ ) {
           var marker = new google.maps.Marker({position: locations[i].latlng,map:map, title:locations[i].name});

       }

       // var point = new google.maps.LatLng(25.801579, -80.202176);

     


       // var marker = new google.maps.Marker({
       //     position:point,
       //     map:map,
       // })

       var contentstring = '<div> Sup</div>';
       var infowindow = new google.maps.InfoWindow({
           content:contentstring
       })

       google.maps.event.addListener(marker,'click',function(){

         infowindow.open(map,marker);


       });
}

     

     google.maps.event.addDomListener(window, 'load', initialize);
   </script>

